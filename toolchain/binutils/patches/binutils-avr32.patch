+enum avr32_arch {
+  ARCH_TYPE_AP,
+  ARCH_TYPE_UCR1,
+  ARCH_TYPE_UCR2,
+  ARCH_TYPE_UCR3,
+  ARCH_TYPE_UCR3FP
+};
+
