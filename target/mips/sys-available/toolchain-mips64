config ADK_TARGET_SYSTEM_TOOLCHAIN_MIPS64
	bool "Toolchain only (mips64 big endian o32 ABI)"
	select ADK_mips
	select ADK_big
	select ADK_o32
	select ADK_toolchain_mips64
