#!/bin/sh
#PKG tinyproxy
#INIT 70

. /etc/rc.conf

case $1 in
autostop) ;;
autostart)
	test x"${tinyproxy:-NO}" = x"NO" && exit 0
	exec sh $0 start
	;;
start)
	tinyproxy
	;;
stop)
	pkill tinyproxy
	;;
restart)
	sh $0 stop
	sh $0 start
	;;

*)
	echo "usage: $0 {start | stop | restart}"
	exit 1
esac
exit $?
