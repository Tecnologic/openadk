config ADK_PACKAGE_APR
	prompt "apr............................... Apache Portable Runtime"
	tristate
	default n
	help
	  Apache Portable Runtime

	  http://apr.apache.org

config ADK_PACKAGE_APR_THREADING
        prompt "Enable threading support"
        bool
        default n
        depends on ADK_PACKAGE_APR
        help
	  Enable threading support in APR.
