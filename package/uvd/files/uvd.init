#!/bin/sh
#PKG uvd
#INIT 90

. /etc/rc.conf

case $1 in
autostop) ;;
autostart)
	test x"${uvd:-NO}" = x"NO" && exit 0
	exec sh $0 start
	;;
start)
	/usr/sbin/uvd
	;;
stop)
	kill $(pgrep -f /usr/sbin/uvd)
	;;
restart)
	sh $0 stop
	sh $0 start
	;;
*)
	echo "usage: $0 (start|stop|restart)"
	exit 1
esac
exit $?
