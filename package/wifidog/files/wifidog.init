#!/bin/sh
#FWINIT 65
. /etc/rc.conf

case $1 in
autostop) ;;
autostart)
	test x"${wifidog:-NO}" = x"NO" && exit 0
	exec sh $0 start
	;;
start)
	wifidog-init start
	;;
stop)
	wifidog-init stop
	;;
restart)
	sh $0 stop
	sh $0 start
	;;
*)
	echo "Usage: $0 {start | stop | restart}"
	exit 1
	;;
esac
exit $?
