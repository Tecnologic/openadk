#!/bin/sh
# disable kernel messages
if [ -f /proc/sys/kernel/printk ];then
	echo 0 > /proc/sys/kernel/printk
fi
echo "System initialization ..."
export PATH=/bin:/sbin:/usr/bin:/usr/sbin
{
	grep '^#INIT ' /etc/init.d/* | \
	    sort -nk2 | \
	    while read line; do
		sh ${line%%:*} autostart
	    done
} | logger -s -p 6 -t ''
