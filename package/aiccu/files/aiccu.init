#!/bin/sh
#PKG aiccu
#INIT 60
. /etc/rc.conf

case $1 in
autostop) ;;
autostart)
	test x"${aiccu:-NO}" = x"NO" && exit 0
	exec sh $0 start
	;;
start)
	aiccu start	
	;;
stop)
	aiccu stop
	;;

restart)
	sh $0 stop
	sh $0 start
	;;
*)
	echo "Usage: $0 {start | stop | restart}"
	exit 1
	;;
esac
exit $?
