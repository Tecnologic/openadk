#!/bin/sh
#PKG busybox
#INIT 05
. /etc/rc.conf

case $1 in
autostop) ;;
autostart)
	[[ $syslogd_flags = NO ]] && exit 0
	exec sh $0 start
	;;
start)
	syslogd $syslogd_flags
	klogd
	;;
stop)
	killall klogd
	killall syslogd
	;;
restart)
	sh $0 stop
	sh $0 start
	;;
*)
	echo "Usage: $0 {start | stop | restart}"
	exit 1
	;;
esac
exit $?
