#!/bin/sh
#PKG nfacctd
#INIT 80

. /etc/rc.conf

case $1 in
autostop) ;;
	autostart)
		test x"${nfacctd:-NO}" = x"NO" && exit 0
		exec sh $0 start
		;;
	start)
		nfacctd $nfaccd_flags
		;;
	stop)
		killall nfacctd
		;;
	restart)
		sh $0 stop
		sh $0 start
		;;
	*)
		echo "usage: $0 {start | stop | restart}"
		exit 1
esac
exit $?
