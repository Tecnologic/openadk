#!/bin/sh
#PKG frickin
#INIT 75
. /etc/rc.conf

case $1 in
autostop) ;;
autostart)
	test x"${frickin:-NO}" = x"NO" && exit 0
	exec sh $0 start
	;;
start)
	/usr/sbin/frickin ${frickin_flags}
	;;
stop)
	kill $(pgrep -f /usr/sbin/frickin)
	;;
restart)
	sh $0 stop
	sh $0 start
	;;
*)
	echo "Usage: $0 {start | stop | restart}"
	exit 1
	;;
esac
exit $?
