#!/bin/sh
#PKG p910nd
#INIT 70
. /etc/rc.conf

case $1 in
autostop) ;;
autostart)
	test x"${p910nd:-NO}" = x"NO" && exit 0
	exec sh $0 start
	;;
start)
	/usr/sbin/p910nd $p910nd_flags
	;;
stop)
	kill $(pgrep -f /usr/sbin/p910d)
	;;
restart)
	sh $0 stop
	sh $0 start
	;;
*)
	echo "Usage: $0 {start | stop | restart}"
	exit 1
	;;
esac
exit $?
